neg := neg-adv-lex &
  [ STEM < "neg" >,
    SYNSEM.LKEYS.KEYREL.PRED "_neg_r_rel" ] .

co_1 := conj-lex &
  [ STEM < "co" >,
    SYNSEM.LKEYS.KEYREL.PRED "_and_coord_rel",
    CFORM "1" ] .

;;; Nouns

n1 := obl-spr-noun-lex &
  [ STEM < "n1" >,
    SYNSEM.LKEYS.KEYREL.PRED "_n1_n_rel" ] .

n2 := noun-lex &
  [ STEM < "n2" >,
    SYNSEM.LKEYS.KEYREL.PRED "_n2_n_rel" ] .

;;; Verbs

iv := finite-intrans-verb-lex &
  [ STEM < "iv" >,
    SYNSEM.LKEYS.KEYREL.PRED "_iv_v_rel" ] .

iv-nf := non-finite-intrans-verb-lex &
  [ STEM < "iv-nf" >,
    SYNSEM.LKEYS.KEYREL.PRED "_iv_v_rel" ] .

tv := finite-trans-verb-lex &
  [ STEM < "tv" >,
    SYNSEM.LKEYS.KEYREL.PRED "_tv_v_rel" ] .

tv-nf := non-finite-trans-verb-lex &
  [ STEM < "tv-nf" >,
    SYNSEM.LKEYS.KEYREL.PRED "_tv_v_rel" ] .

;;; Auxiliaries

aux := subj-raise-aux-no-sem &
  [ STEM < "aux" > ] .

;;; Other

;;; Determiners

det := determiner-lex &
  [ STEM < "det" >,
    SYNSEM.LKEYS.KEYREL.PRED "_det_q_rel" ] .

sel-adv := neg-adv-lex &
  [ STEM < "sel-adv" >,
    SYNSEM.LKEYS.KEYREL.PRED "_neg_r_rel" ] .

